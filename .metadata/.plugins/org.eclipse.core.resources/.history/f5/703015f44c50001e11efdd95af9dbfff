package sp1;

import java.util.ArrayList;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class dto_product {
	String pidx, pcode, pname, pmoney, pimg, pasle, puse ;
	
	public ArrayList<String> db_product(){
		ArrayList<String> dp = new ArrayList<String>();
		
		dp.add(getPidx());
		dp.add(getPcode());
		dp.add(getPname());
		dp.add(getPmoney());
		dp.add(getPimg());
		dp.add(getPasle());
		dp.add(getPuse());
		return dp;
	}
}
