package sp1;

import java.util.ArrayList;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class dto_product {
	String pidx, pcode, pname, pmoney, pimg, pasle, puse ;
	
	public ArrayList<String> db_product(){
		ArrayList<String> dp = new ArrayList<String>();
		
		dp.add(getPidx());	//자동증가값 0 
		dp.add(getPcode());	//1
		dp.add(getPname());	//2
		dp.add(getPmoney());//3
		dp.add(getPuse());//4
		return dp;
	}
}
